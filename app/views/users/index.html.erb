<% provide(:title, 'All users') %>
    <div class="row">
      <aside class="col-md-4">
        <section class="user_info">
            <%= render 'shared/user_info' %>
        </section>
      </aside>
      <div class="col-md-8">
        <%= will_paginate %>
        <ul class= "users">
          <%= render partial: 'users/user', collection: @users %>
        </ul>
        <%= will_paginate %>
      </div>
    </div>
